
<div class="page-header">
  <h1>Login</h1>
</div>



<%= simple_form_for :user, :html => { :class => 'form-horizontal' } do |f| %>

  <%= f.input :login %>
  <%= f.input :password %>


  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary', :value => 'Login' %>
  </div>
<% end %>